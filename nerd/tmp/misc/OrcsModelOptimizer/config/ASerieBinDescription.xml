		<value binPos="108" orcs="/Sim/ASeries/Left/AnklePitch/JointAngle" />
		<value binPos="111" orcs="/Sim/ASeries/Left/AnklePitch/DesiredAngle" />

		<!-- Left AnkleYaw -->
