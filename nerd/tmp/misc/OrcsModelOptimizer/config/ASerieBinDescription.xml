<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration>
	<physicalDataSourceDescription name="Binary">
	
		<normalization physicalMin="-32768" physicalMax="32767"/>

		<!-- 	timePos = position in frame for timestamp 
			timeChangeIntervall = number of frames before the timestamp changes
			frameLength = length of a frame in byte
			frameTimeLength = time in msec between the frames
			syncValue = value of the first value in the frame	-->
		<dataFormat 	timePos="2"		
				timeChangeIntervall="10"
				frameLength="128" 
				frameTimeLength="10"
				syncValue="21845"/>

		<!-- Head Pan -->
		<value binPos="17" orcs="/Sim/ASeries/Head/Pan/DesiredTorque" />
		<value binPos="27" orcs="/Sim/ASeries/Head/Pan/JointAngle" />
		<value binPos="30" orcs="/Sim/ASeries/Head/Pan/DesiredAngle" />
	
		<!-- Head Tilt -->
		<value binPos="18" orcs="/Sim/ASeries/Head/Tilt/DesiredTorque" />	
		<value binPos="28" orcs="/Sim/ASeries/Head/Tilt/JointAngle" />
		<value binPos="31" orcs="/Sim/ASeries/Head/Tilt/DesiredAngle" />

		<!-- Left AnklePitch -->
